<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp" >

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_price"
        android:text="요청사항" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_weight"
        android:text="배송비" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_howMany"
        android:text="총중량" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_box"
        android:text="포장수량" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_product"
        android:text="포장종류" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_arriveAddr"
        android:text="제품종류" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/detail_startAddr"
        android:text="도착지" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/detail_startAddr"
        android:layout_marginStart="17dp"
        android:text="출발지" />

    <TextView
        android:id="@+id/detail_startAddr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="74dp"
        android:layout_marginTop="11dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_arriveAddr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_startAddr"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_arriveAddr"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_product"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_howMany"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_box"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_howMany"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_weight"
        android:text="TextView" />

    <TextView
        android:id="@+id/detail_memo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/detail_startAddr"
        android:layout_below="@+id/detail_price"
        android:text="TextView" />

    <GridView
        android:id="@+id/detail_image"
        android:layout_width="wrap_content"
        android:layout_height="326dp"
        android:layout_below="@id/detail_memo"
        android:layout_marginTop="20dp"></GridView>

</RelativeLayout>